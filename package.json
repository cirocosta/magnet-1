{
  "name": "magnet",
  "version": "0.1.0",
  "description": "The API framework for nodejs.",
  "main": "build/magnet.js",
  "scripts": {
    "build": "./node_modules/.bin/gulp build",
    "lint": "./node_modules/.bin/gulp lint",
    "release": "NODE_ENV=production ./node_modules/.bin/gulp build",
    "test": "./node_modules/.bin/gulp coverage",
    "test:watch": "./node_modules/.bin/gulp test:watch",
    "test:coverage:travis": "./node_modules/.bin/gulp test:coverage:travis",
    "generate-changelog": "github-changes --owner wedeploy --repository magnet -a --only-pulls --use-commit-body",
    "preversion": "npm test",
    "version": "npm run generate-changelog && git add CHANGELOG.md",
    "prepublish": "npm run build"
  },
  "author": "Ivan Santos <ivan.santos@liferay.com>",
  "license": "BSD-3-Clause",
  "keywords": [
    "magnet",
    "api",
    "rest",
    "express"
  ],
  "bugs": {
    "url": "https://github.com/wedeploy/magnet/issues"
  },
  "bin": {
    "magnet": "build/bin/magnet"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wedeploy/magnet.git"
  },
  "homepage": "https://github.com/wedeploy/magnet",
  "dependencies": {
    "bluebird": "^3.4.7",
    "body-parser": "^1.13.1",
    "chokidar": "^1.6.1",
    "compression": "^1.6.2",
    "cross-spawn": "^5.0.1",
    "express": "^4.13.0",
    "express-wizard": "^0.2.4",
    "helmet": "^3.3.0",
    "lodash": "^4.17.4",
    "lx-valid": "^1.2.4",
    "minimist": "^1.2.0",
    "morgan": "^1.6.0",
    "nodemon": "^1.11.0",
    "winston": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-async-to-module-method": "^6.22.0",
    "babel-plugin-transform-promise-to-bluebird": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "co-mocha": "^1.1.3",
    "deep-merge": "^1.0.0",
    "eslint": "^3.13.1",
    "eslint-config-google": "^0.7.1",
    "eslint-loader": "^1.6.1",
    "github-changes": "^1.0.4",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.1",
    "gulp-codecov": "^2.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^3.0.1",
    "gulp-istanbul": "^1.1.1",
    "gulp-load-plugins": "^1.4.0",
    "gulp-mocha": "^3.0.1",
    "gulp-notify": "^3.0.0",
    "isparta": "^4.0.0",
    "mocha": "^3.2.0",
    "run-sequence": "^1.2.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "supertest": "^2.0.1",
    "webpack": "^2.2.0"
  }
}
